<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle" >
            <corners
                android:radius="20dp"
                />
            <solid
                android:color="#89a5fd"
                />
            <size
                android:width="20dp"
                android:height="20dp"
                />
            <padding
                android:top="0.5dp"
                android:bottom="1dp"
            />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle" >
            <corners
                android:radius="20dp"
                />
            <solid
                android:color="#768fdb"
                />
            <size
                android:width="20dp"
                android:height="20dp"
                />
            <padding
                android:top="0.5dp"
                android:bottom="1dp"
                />
        </shape>
    </item>
</selector>